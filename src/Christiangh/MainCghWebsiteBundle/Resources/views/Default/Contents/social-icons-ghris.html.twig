        <li><img src="{{ asset('contents/social-icons-ghris/images/facebook.png') }}" title="Facebook" alt="Facebook {{ 'content_social_icons_ghris.icon'|trans([], 'content_social_icons_ghris') }}" /></li>
        <li><img src="{{ asset('contents/social-icons-ghris/images/twitter.png') }}" title="Twitter" alt="Twitter {{ 'content_social_icons_ghris.icon'|trans([], 'content_social_icons_ghris') }}" /></li>
        <li><img src="{{ asset('contents/social-icons-ghris/images/google-plus.png') }}" title="Google +" alt="Google + {{ 'content_social_icons_ghris.icon'|trans([], 'content_social_icons_ghris') }}" /></li>
