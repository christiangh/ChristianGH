    <p id="info">*{{ 'content_social_icons_ghris.info'|trans([], 'content_social_icons_ghris')|raw }}</p>
    <p id="message">{{ 'content_social_icons_ghris.message'|trans([], 'content_social_icons_ghris')|raw }}</p>
</div>
{% endblock %}

{% block javascripts %}
{{ parent() }}
<script src="{{ asset('contents/social-icons-ghris/js/javascript.js') }}" type="text/javascript"></script>
{% endblock %}